[project]
	description = Gin is a web framework written in Golang. It features a Martini-like API with much better performance -- up to 40 times faster. If you need smashing performance, get yourself some Gin.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Robert Wilkinson (RobAWilkinson)
[access "refs/for/refs/*"]
	read = group user/Robert Wilkinson (RobAWilkinson)
	push = group user/Robert Wilkinson (RobAWilkinson)
	label-Code-Review = -2..+2 group user/Robert Wilkinson (RobAWilkinson)
	label-Verified = -1..+1 group user/Robert Wilkinson (RobAWilkinson)
	rebase = group user/Robert Wilkinson (RobAWilkinson)
	removeReviewer = group user/Robert Wilkinson (RobAWilkinson)
	submit = group user/Robert Wilkinson (RobAWilkinson)
[access "refs/heads/*"]
	read = group user/Robert Wilkinson (RobAWilkinson)
	create = group user/Robert Wilkinson (RobAWilkinson)
	push = +force group user/Robert Wilkinson (RobAWilkinson)
	pushMerge = group user/Robert Wilkinson (RobAWilkinson)
[access "refs/tags/*"]
	push = +force group user/Robert Wilkinson (RobAWilkinson)
	pushTag = group user/Robert Wilkinson (RobAWilkinson)
	pushSignedTag = group user/Robert Wilkinson (RobAWilkinson)
